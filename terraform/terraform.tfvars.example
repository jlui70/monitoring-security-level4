# AWS Configuration
aws_region         = "us-east-1"

# EC2 Instance
ec2_instance_type  = "t3.medium"
ec2_key_name       = "your-key-name-here"  # Replace with your AWS key pair name

# Security - SSH Access
# IMPORTANT: Restrict this to your IP for production!
allowed_ssh_ips    = ["0.0.0.0/0"]  # Replace with ["YOUR_IP/32"]

# Instructions:
# 1. Copy this file to terraform.tfvars
# 2. Replace the values with your own
# 3. Run: terraform init && terraform apply
